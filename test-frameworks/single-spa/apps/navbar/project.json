{
  "name": "navbar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "navbar",
  "sourceRoot": "apps/navbar/src",
  "tags": [],
  "targets": {
    "build": {
      "baseHref": "/",
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/navbar",
        "index": "apps/navbar/src/index.html",
        "main": "apps/navbar/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/navbar/tsconfig.app.json",
        "inlineStyleLanguage": "less",
        "assets": [
          "apps/navbar/src/favicon.ico",
          "apps/navbar/src/assets"
        ],
        "styles": [
          "apps/navbar/src/styles.less"
        ],
        "scripts": [
//         "apps/navbar/src/assets/minified.js",
//         "apps/navbar/src/assets/zone.js",
//         "apps/navbar/src/assets/import-map-overrides.js",
//         "apps/navbar/src/assets/system.min.js",
//         "apps/navbar/src/assets/amd.min.js",
//         "apps/navbar/src/assets/minified.js",
//         "apps/navbar/src/assets/named-exports.js",
//         "apps/navbar/src/assets/named-register.min.js"
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/navbar/src/environments/environment.ts",
              "with": "apps/navbar/src/environments/environment.prod.ts"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "none"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "servePath": "/"
      },
      "configurations": {
        "production": {
          "buildTarget": "navbar:build:production"
        },
        "development": {
          "buildTarget": "navbar:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "navbar:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/navbar/jest.config.ts"
      }
    }
  }
}
